{
	"window.commandCenter": true,
	"workbench.colorTheme": "GitHub Light Default",
	"editor.fontFamily": "'SauceCodePro Nerd Font', 'Consolas Nerd Font', Monaco, 'Courier New', monospace",
	"editor.fontSize": 14,
	"editor.formatOnSave": true,
	"explorer.confirmDelete": false,
	"gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm",
	"gitlens.defaultDateStyle": "absolute",
	"editor.minimap.enabled": true,
	"git.enableSmartCommit": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"extensions.ignoreRecommendations": true,
	"[yaml]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": "advanced",
		"diffEditor.ignoreTrimWhitespace": false,
		"editor.defaultColorDecorators": "never"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"workbench.editor.pinnedTabsOnSeparateRow": true,
	"vim.neovimUseConfigFile": true,
	"editor.lineNumbers": "relative",
	"python.analysis.typeCheckingMode": "standard",
	"terminal.integrated.enableMultiLinePasteWarning": "never",
	"git.confirmSync": false,
	"github.copilot.nextEditSuggestions.enabled": true,
	"workbench.activityBar.location": "top",
	"chat.commandCenter.enabled": false,
	"cSpell.enabledFileTypes": {
		"shellscript": false,
		"json": false,
		"yaml": false
	},
	"cSpell.ignorePaths": [
		"package-lock.json",
		"node_modules",
		"vscode-extension",
		".git/{info,lfs,logs,refs,objects}/**",
		".git/{index,*refs,*HEAD}",
		".vscode",
		".vscode-insiders"
	],
	"cSpell.userWords": [
		"admob",
		"asgi",
		"asyncio",
		"cpython",
		"dssm",
		"fastapi",
		"httptools",
		"kaisou",
		"mori",
		"msgpack",
		"olap",
		"pixai",
		"Pydantic",
		"Reco",
		"rerank",
		"reranked",
		"Reranker",
		"shumei",
		"Tanebi",
		"UMAP",
		"uvicorn",
		"uvloop",
		"venv"
	],
	"files.defaultLanguage": "json",
	"vim.autoSwitchInputMethod.enable": true,
	"vim.autoSwitchInputMethod.defaultIM": "com.apple.keylayout.ABC",
	"vim.autoSwitchInputMethod.obtainIMCmd": "/opt/homebrew/bin/macism",
	"vim.autoSwitchInputMethod.switchIMCmd": "/opt/homebrew/bin/macism {im}"
}
