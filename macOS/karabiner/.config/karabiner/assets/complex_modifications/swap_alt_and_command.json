{
  "title": "Swap ALT and Command",
  "rules": [
    {
      "description": "Swap option and command",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_option",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "org.alacritty",
                "com.valvesoftware.dota2",
                "com.microsoft.VSCode",
                "dev.zed.Zed"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_option"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "org.alacritty",
                "com.valvesoftware.dota2",
                "com.microsoft.VSCode",
                "dev.zed.Zed"
              ]
            }
          ]
        }
      ]
    }
  ]
}
