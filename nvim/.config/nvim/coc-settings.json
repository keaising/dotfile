{
  "sumneko-lua.enableNvimLuaDev": true,
  "Lua": {
    "workspace": {
      "checkThirdParty": false,
      "library": ["${3rd}/luv/libray"]
    },
    "hint.enable": false
  },
  "ruff.autoFixOnSave": true,
  "git": {
    "addGBlameToVirtualText": true
  },
  "coc": {
    "preferences": {
      "colorSupport": true
    },
    "source": {
      "emoji": {
        "filetypes": null,
        "triggerCharacters": ["/"]
      },
      "word.priority": -1,
      "dictionary.priority": -1
    }
  },
  "diagnostic": {
    "errorSign": "",
    "warningSign": "",
    "infoSign": "",
    "hintSign": ""
  },
  "snippets": {
    "ultisnips": {
      "directories": ["$HOME/.config/nvim/ultisnips"],
      "pythonPrompt": false
    }
  },
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
